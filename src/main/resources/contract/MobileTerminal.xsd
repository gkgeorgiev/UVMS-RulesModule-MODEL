<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:mobterm="urn:mobileterminal.rules.schema.fisheries.ec.europa.eu:v1" 
            targetNamespace="urn:mobileterminal.rules.schema.fisheries.ec.europa.eu:v1"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
            xmlns="http://schemas.xmlsoap.org/wsdl/">
    
    <xsd:complexType name="MobileTerminalId">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="MobileTerminal">
        <xsd:sequence>
            <xsd:element name="id" type="mobterm:MobileTerminalId"/>
            <xsd:element name="connectId" type="xsd:string"/>
            <xsd:element name="type" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="mobileTerminalId" type="mobterm:Id" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:simpleType name="IdType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="SATELLITE_NUMBER"/>
            <xsd:enumeration value="DNID"/>
            <xsd:enumeration value="MEMEBER_NUMBER"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:complexType name="Id">
        <xsd:sequence>
            <xsd:element name="type" type="mobterm:IdType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="value" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>